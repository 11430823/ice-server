<route>
  <!--
  name:库名
  type:分库类型(100:0-99,100个库)
  -->
  <dbser name="ICE" type="100">
    <cmd>
      <!--
      start:起始CMD_ID
      end:结束CMD_ID
      -->
      <date start="12" end="34" />
      <date start="56" end="78" />
    </cmd>
    <db>
      <date name="dbser" ip="10.1.1.57" port="1234" start="0" end="20"/>
      <date name="dbser" ip="10.1.1.57" port="1234" start="21" end="40"/>
      <date name="dbser" ip="10.1.1.57" port="1234" start="41" end="99"/>      
    </db>
  </dbser>
  <dbser name="ICE00" db_type="100">
    <cmd>
      <!--
      start:起始CMD_ID
      end:结束CMD_ID
      -->
      <date start="1200" end="3400" />
      <date start="5600" end="7800" />
    </cmd>
    <db>
      <date name="dbser00" ip="10.1.1.57" port="1234" start="0" end="20"/>
      <date name="dbser00" ip="10.1.1.57" port="1234" start="21" end="40"/>
      <date name="dbser00" ip="10.1.1.57" port="1234" start="41" end="99"/>
    </db>
  </dbser>
</route>
