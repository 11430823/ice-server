[plugin]
liblogic=./liblogic.so
[common]
#子进程打开FD的最大值,默认20000
#max_fd_num=100
#默认为1:守护进程模式
#is_daemon=1
#连接FD超时秒数.默认0:连接FD无超时
#fd_time_out=0
#数据包最大字节数.默认8192
#page_size_max=8192
[log]
#日志目录
dir=../log
#日志等级.默认8
#level=8
#每多少时间(分钟)重新保存文件中(新文件).0的话为普通日志方式.默认0
#save_next_file_interval_min=15
[core]
#文件大小(字节).默认1073741824*2:2GB
#size=1073741824
#最大重启次数.默认20
#restart_cnt_max=10
[net]
#服务IP.守护进程的链接信息(注释掉表明不需要链接)
#daemon_tcp_ip=192.168.1.19
#服务端口号,默认9999
#daemon_tcp_port=9999
#服务名称
#daemon_name=daemon_fire
#打开FD的最大值,默认20000
#daemon_tcp_max_fd_num=20000

